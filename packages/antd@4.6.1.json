{
  "name": "antd",
  "slug": "antd@4.6.1",
  "isDesignSystem": true,
  "components": {
    "Dropdown": {
      "name": "Dropdown",
      "namedImport": true,
      "hasChildElements": false,
      "props": {
        "arrow": {
          "propName": "arrow",
          "type": "boolean",
          "defaultValue": false,
          "context": "Whether the dropdown arrow should be visible"
        },
        "disabled": {
          "propName": "disabled",
          "type": "boolean",
          "defaultValue": false,
          "context": "Whether the dropdown menu is disabled"
        },
        "placement": {
          "propName": "placement",
          "type": "string",
          "defaultValue": "bottomLeft",
          "options": [
            "bottomLeft",
            "bottomRight",
            "bottomCenter",
            "topLeft",
            "topCenter",
            "topRight"
          ],
          "context": "Placement of popup menu"
        },
        "visible": {
          "propName": "visible",
          "type": "boolean",
          "defaultValue": false,
          "context": "Whether the dropdown menu is currently visible"
        }
      }
    },
    "Button": {
      "name": "Button",
      "namedImport": true,
      "hasChildElements": true,
      "props": {
        "type": {
          "propName": "type",
          "type": "string",
          "defaultValue": "default",
          "options": [
            "primary",
            "ghost",
            "dashed",
            "dangeer",
            "link",
            "text",
            "default"
          ],
          "context": "Used to set the type of button"
        },
        "size": {
          "propName": "size",
          "type": "string",
          "options": ["large", "middle", "small"],
          "context": "Set the size of the button"
        },
        "target": {
          "propName": "target",
          "type": "string",
          "context": "Same as target attribute of a, works when href is specified"
        },
        "shape": {
          "propName": "shape",
          "type": "string",
          "options": ["circle", "round"],
          "context": "Can be used to set the shape of the button or omitted"
        },
        "loading": {
          "propName": "loading",
          "type": "boolean",
          "defaultValue": false,
          "context": "Used in setting the loading state of button"
        },
        "htmlType": {
          "propName": "htmlType",
          "type": "string",
          "defaultValue": "button",
          "context": "Set the original html type of button. see the MDN docs"
        },
        "href": {
          "propName": "href",
          "type": "string",
          "context": "Redirect url of link button"
        },
        "ghost": {
          "propName": "ghost",
          "type": "string",
          "defaultValue": false,
          "context": "Make background transparent and invert text and border colors"
        },
        "disabled": {
          "propName": "disabled",
          "type": "boolean",
          "defaultValue": false,
          "context": "Disabled state of button"
        },
        "danger": {
          "propName": "danger",
          "type": "boolean",
          "defaultValue": false,
          "context": "Set the danger status of button"
        },
        "block": {
          "propName": "block",
          "type": "boolean",
          "defaultValue": false,
          "context": "Option to fit button width to its parent width"
        }
      }
    }
  },
  "npm": {
    "dependency": {
      "path": "antd",
      "version": "^4.6.1"
    },
    "styles": ["antd/dist/antd.css"]
  },
  "cdn": {
    "dependency": {
      "path": "antd",
      "version": "4.6.1"
    },
    "styles": ["https://unpkg.com/antd@4.6.1/dist/antd.min.css"]
  }
}

{
  "name": "react-bootstrap",
  "version": "1.3.0",
  "isDesignSystem": true,
  "components": {
    "Alert": {
      "name": "Alert",
      "namedImport": true,
      "package": "react-bootstrap",
      "props": {
        "variant": {
          "type": "string",
          "default": "null",
          "context": "Type of Alert style, success, info, warning, error",
          "options": [
            "primary",
            "secondary",
            "success",
            "danger",
            "warning",
            "info",
            "dark",
            "light"
          ]
        }
      }
    },
    "Button": {
      "name": "Button",
      "namedImport": true,
      "package": "react-bootstrap",
      "props": {
        "variant": {
          "type": "string",
          "default": "primary",
          "context": "Can be set to primary, ghost, dashed, danger, link, text, default",
          "options": [
            "primary",
            "secondary",
            "succeess",
            "link",
            "warning",
            "danger",
            "outline-primary",
            "outline-secondary",
            "online-success"
          ]
        },
        "disabled": {
          "type": "boolean",
          "default": false,
          "context": "Disables the button, prevents mouse eveents, eeven if the underlying component is an <a> eeleement"
        },
        "size": {
          "type": "string",
          "default": null,
          "context": "Can be set to circle, round or omitted",
          "options": ["sm", "lg"]
        },
        "type": {
          "type": "string",
          "default": "button",
          "context": "Defined HTML button type attribute",
          "options": ["button", "reset", "submit"]
        }
      }
    }
  }
}
